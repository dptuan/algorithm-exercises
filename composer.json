{
    "name": "tuando/skeleton-php",
    "description": "A skeleton repository for my packages",
    "keywords": ["php", "skeleton", "package"],
    "license": "MIT",
    "authors": [
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "pestphp/pest": "^0.2.2"
    },
    "autoload": {
        "psr-4": {
            "TuanDo\\SkeletonPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test:unit": "pest --colors=always",
        "test": [
            "@test:unit"
        ]
    }
}
